{
	"maxJobDurationInSeconds": 600,
	"iamRole": "arn:aws:iam::629712188062:role/RoboMakerGeneralRole",
	"robotApplications": [
		{
			"application": "arn:aws:robomaker:eu-west-1:629712188062:robot-application/rigel_robomaker_robot_application/1689714131091",
			"applicationVersion": "$LATEST",
			"launchConfig": {
				"environmentVariables": {
					"ROS_IP": "ROBOMAKER_ROBOT_APP_IP",
					"ROS_MASTER_URI": "http://ROBOMAKER_ROBOT_APP_IP:11311",
					"GAZEBO_MASTER_URI": "http://ROBOMAKER_SIM_APP_IP:11345"
				},
				"streamUI": true,
				"command": [
					"roslaunch", "t2rgb_bringup", "app_launch.launch", "motion_sequence_file:=/home/rigeluser/ros_workspace/src/t2rgb/movement_sequences/movement_sequence.json", "output_folder:=/home/rigeluser/ros_workspace/src/t2rgb/datasets/new_dataset", "overwrite:=true"
				]
			},
			"tools": [
				{
					"streamUI": true,
					"name": "terminal",
					"command": "source /home/rigeluser/robot-entrypoint.sh && xfce4-terminal",
					"streamOutputToCloudWatch": true,
					"exitBehavior": "RESTART"
				},
				{
					"streamUI": true,
					"name": "gzclient",
					"command": "source /home/rigeluser/robot-entrypoint.sh && gzclient",
					"streamOutputToCloudWatch": true,
					"exitBehavior": "RESTART"
				},
				{
					"streamUI": true,
					"name": "rviz",
					"command": "source /home/rigeluser/robot-entrypoint.sh && rviz",
					"streamOutputToCloudWatch": true,
					"exitBehavior": "RESTART"
				}
			]
		}
	]
}
